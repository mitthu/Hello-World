#!/bin/bash
# Date of Creation: 18-July-2014

# Include my common libraries
source $HOME/.me/include

# Update path location
export PATH=~/.me/bin:$PATH:~/.bin:~/.local/bin
export PATH=$PATH:/usr/java/default/bin

# Add support for 256 colors
export TERM=xterm-256color

# Set the default editor as vim
export EDITOR=vim

# Misc.
export TZ=Asia/Kolkata

# Personal hosts file
export HOSTALIASES=~/.hosts

# Source all the local bash rc's
source ~/.melocal/*

# Unset all temperory variables
unset_me
